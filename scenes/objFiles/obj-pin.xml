		<Object type="BarycentricMapping" />
	</Node>

	<Node name="Visu">
		<Object type="VisualModel" name="Visual" filename="PinAqua.obj"  color="red" />
		<Object type="BarycentricMapping" object1="../.." object2="Visual" />
